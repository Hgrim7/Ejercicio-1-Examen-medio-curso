# Ejercicio-1-Examen-medio-curso
//inciso a)
size(100,100);
background(255);
fill(0);
ellipse(25,25,10,10);
ellipse(25,75,10,10);
ellipse(75,25,10,10);
ellipse(75,75,10,10);

//Inciso b)
size(100,100);
background(255);
int x= 25;
int y= 10;
int c= 75;
fill(0);
ellipse(x,x,y,y);
ellipse(x,c,y,y);
ellipse(c,x,y,y);
ellipse(c,c,y,y);
